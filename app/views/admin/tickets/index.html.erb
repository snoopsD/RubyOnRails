<h1>Билеты </h1>
  <tbody>
      <% @tickets.each do |ticket| %>
        <tr>
          <td><%= link_to ticket.id, admin_ticket_path(ticket) %></td>
          <td><%= link_to 'Редактировать', edit_admin_ticket_path(ticket) %></td>
          <td><%= link_to 'Удалить', ticket, method: :delete, data: { confirm: 'Are you sure?' } %></td> <br>
        </tr>
      <% end %>
    </tbody> <br />
<%= link_to 'На главную', admin_index_path %>
